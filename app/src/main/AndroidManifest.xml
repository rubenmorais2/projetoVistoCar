<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ruben.vistocar">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_vistocar_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_vistocar_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DebugActivity"/>
        <activity android:name=".Prefs" />
        <activity android:name=".LMSApplication" />
        <activity android:name=".CriarAgendamento" />
        <activity android:name=".agendamento" />
        <activity android:name=".TelaInicial" android:theme="@style/AppTheme.Toolbar"></activity>
    </application>

</manifest>